{
  "name": "@talimy/database",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index",
  "types": "./src/index",
  "scripts": {
    "db:generate": "bunx drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "bun run src/migrate.ts",
    "db:seed": "bun run src/seed/index.ts",
    "db:studio": "bunx drizzle-kit studio --config=drizzle.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "drizzle-orm": "0.38.3",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/pg": "^8.11.11",
    "drizzle-kit": "0.30.1",
    "typescript": "5.9.3"
  }
}
